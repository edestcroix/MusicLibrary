{
  "app-id": "ca.edestcroix.MusicLibary",
  "runtime": "org.gnome.Platform",
  "runtime-version": "45",
  "sdk": "org.gnome.Sdk",
  "command": "musiclibrary",
  "finish-args": [
    "--share=network",
    "--share=ipc",
    "--socket=fallback-x11",
    "--device=dri",
    "--socket=wayland"
  ],
  "cleanup": [
    "/include",
    "/lib/pkgconfig",
    "/man",
    "/share/doc",
    "/share/gtk-doc",
    "/share/man",
    "/share/pkgconfig",
    "*.la",
    "*.a"
  ],
  "modules": [
    "pypy-dependencies.json",
    {
      "name": "tiff",
      "buildsystem": "cmake",
      "builddir": true,
      "versions": [
        "<= 4.5.1"
      ],
      "sources": [
        {
          "type": "archive",
          "url": "https://download.osgeo.org/libtiff/tiff-4.4.0.tar.gz",
          "sha256": "917223b37538959aca3b790d2d73aa6e626b688e02dcda272aec24c2f498abed"
        }
      ]
    },
    {
      "name": "musiclibrary",
      "builddir": true,
      "buildsystem": "meson",
      "sources": [
        {
          "type": "git",
          "url": "file:///var/home/yelgo/Projects"
        }
      ]
    }
  ]
}
